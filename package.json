{
  "name": "tailwind-nextjs-controls-starter",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "clsx": "^1.1.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "^4.0.1",
    "typescript": "^4.0.3",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "prestart": "npm run tailwind:dev",
    "prebuild": "npm run tailwind:build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test:create": "cc-cli -c .storybook -g store -o tests -f ts -w",
    "test": "yarn jest",
    "eject": "react-scripts eject",
    "tailwind:build": "NODE_ENV=production postcss ./src/styles/tailwind.css -o ./src/tailwind.output.css",
    "tailwind:dev": "postcss ./src/styles/tailwind.css -o ./src/tailwind.output.css",
    "tailwind:watch": "postcss ./src/styles/tailwind.css -o ./src/tailwind.output.css --watch",
    "docs:build": "next build && next export",
    "docs:start": "next -p 6006",
    "lint": "eslint \"./src/**/*.{ts,tsx,js,jsx}\"",
    "lint:fix": "eslint --fix \"./src/**/*.{ts,tsx,js,jsx}\"",
    "format": "prettier --write \"./src/**/*.{ts,tsx,js,jsx,json,md}\""
  },
  "lint-staged": {
    "*.{ts, tsx, js, jsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "CI=true npm run test"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@component-controls/cc-cli": "^3.5.5",
    "@component-controls/nextjs-plugin": "^3.5.5",
    "@fullhuman/postcss-purgecss": "^3.0.0",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.6",
    "@types/jest": "^26.0.22",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/testing-library__jest-dom": "^5.9.5",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "eslint": "^7.13.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest-dom": "^3.2.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^3.10.0",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "jest-matchmedia-mock": "^1.1.0",
    "jest-transform-css": "^2.1.0",
    "lint-staged": "^10.5.1",
    "next": "^10.0.8",
    "postcss": "^8.2.7",
    "postcss-cli": "^8.2.0",
    "prettier": "^2.1.2",
    "rc-util": "^5.9.8",
    "react-is": "^17.0.1",
    "tailwindcss": "^1.9.6",
    "ts-jest": "^26.5.4"
  },
  "jest": {
    "preset": "ts-jest",
    "transform": {
      "^.+\\.(ts|tsx)?$": "ts-jest",
      "^.+\\.(js|jsx)$": "babel-jest",
      ".+\\.(css|styl|less|sass|scss)$": "jest-transform-css"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ccontrols/tailwind-nextjs-controls-starter.git"
  },
  "resolutions": {
    "webpack": "5.24.3"
  }
}
